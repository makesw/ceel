/* Inicio Estilos css pantalla de login */

body{color: #fff;background:#12122e;font-family: 'Roboto', sans-serif;}
header{background: #12122e;border-bottom: solid 3px #3d3d9b;}
table{color: black;}
table th{text-align: center;font-size: 0.8em;}
table.dataTable{font-size: 1.2em;}
.container-content{min-height: 600px;}
.logo{font-size: 1.2em;font-weight: 100;padding: 20px 10px 10px 25px;font-style: italic;}
.logo span{color: #8485fc;margin: 0 10px;}
.contact-p a{color: #fff;text-decoration: none;}
.contact-p a:hover{color: #5454a0;}
.contact-p{margin-top: 20px;padding-right:20px;font-weight:100;text-align:right;}
.contact-t-p{margin-left:10px; }
.contact-p .lnr{font-size: 2.2em;vertical-align: middle;}

.contact-p .text-avatar-header{display: inline-block;margin: 0 5px;color: #ffffff;font-size: 1em;vertical-align: middle;font-weight: 400;}
.contact-p .text-avatar-header span{text-transform: uppercase;color: #21e9de;font-weight: 300;}
.contact-p .avatar-header{display: inline-block;margin: 0 5px;}
.contact-p .img-circle{border: 4px solid #21e9de;width: 50px;}
.contact-p .menu{display: inline-block;}
.contact-p button#dropdownmenu1{background: transparent;color: #ffffff;border: none;}
.contact-p button#dropdownmenu1:hover{background: #afafe2;color: #12122e;border-radius: 10px;}
.contact-p button#dropdownmenu1:active{background: #afafe2;}
.contact-p ul.dropdown-menu{left: -130px;background: #afafe2;color: #12122e;border-radius: 0;min-width:190px;margin: 16px 0 0;padding: 0;font-size: 1.1em;}
.contact-p ul.dropdown-menu li:last-child{background:#666687;}
.contact-p ul.dropdown-menu li:last-child a{color: #ffffff!important;text-align: center;font-weight: 500;}
.contact-p ul.dropdown-menu li:last-child a:hover{color: #12122e!important;background: #ffffff;}
.contact-p ul.dropdown-menu .lnr{font-size: 1.4em; vertical-align: middle;padding-right: 8px;}
.contact-p ul.dropdown-menu .fa{font-size: 1.4em; vertical-align: middle;padding-right: 8px;}
.contact-p ul.dropdown-menu .fa-chevron-right{float: right;font-size: 1.2em;width: 10%;}
.contact-p ul.dropdown-menu>li>a:hover{padding:15px 10px;background: #12122e;color: #ffffff;}
.contact-p ul.dropdown-menu>li>a:active{background: #12122e;color: #ffffff;}
.contact-p ul.dropdown-menu>li>a{padding:14px 10px;}

footer {color: #7c7b7b;font-weight: 500;font-size:0.9em;text-align: center;background:#12122e;border-top: solid 3px #34354c;padding: 20px 0;}
footer span a{color: #5454a0;text-decoration: none;}
.b-login{padding-bottom: 10px;background: url(../assets/img/bg-login.png) #12132f no-repeat center center fixed;-webkit-background-size: cover;-moz-background-size: cover;-o-background-size: cover;background-size: cover;}
.form-login{background: #fff;font-size:1.4em;-webkit-border-radius: 5px;-moz-border-radius: 5px;border-radius: 5px;margin: 40px auto;padding: 0 20px 10px 20px;}
.form-login h3{color: #5d5d5d; text-align: center;padding-top: 20px;}
form label{color:#ababab;font-weight: 300;font-size: 0.9em;}
.form-login hr{color: #858585;}
.form-login a{text-decoration: none;}
.form-login a h4{color: #ababab;text-align: center;text-decoration: none;font-size: 0.7em;padding: 10px 0;font-weight: 300;}
.form-login input[type=text], input[type=password], input[type=email] { background: #e4e4e4;font-size: 1em; padding: 20px 10px;text-align: center;color: #858585;}
.contact-admin{text-align: center;font-weight: 500;font-size: 1.1em;margin: 20px 0;}
.contact-admin a{color: #5454a0;text-decoration: none;}
.form-login button.btn.btn-warning {width: 100%;font-size: 1em;}
.content-login{padding:20px 40px;}
.content-login p{font-weight: 300;width: 80%;margin-top: 20px;}
.list-login{color: #8181c0;margin-top:30px;}
.list-login li{margin: 10px 0;}

/* Fin Estilos css pantalla de login */

