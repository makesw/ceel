.upload-img {
  visibility: hidden;
  width: 0;
  position: relative;
}
.upload-img::before {
  content: 'Seleccionar imagen';
  display: inline-block;
  background: #5454a0;
  border-radius: 3px;
  padding: 5px 5px;
  outline: none;
  white-space: nowrap;
  -webkit-user-select: none;
  cursor: pointer;
  font-weight: 300;
  font-size: 10pt;
  visibility: visible;
  position: absolute;
}
.upload-img:hover::before {
  border-color: none;
}



